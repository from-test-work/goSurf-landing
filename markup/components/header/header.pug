include ../main-slider/main-slider
include ../main-subslider/main-subslider

include ../brand/brand
include ../nav-list/nav-list
include ../search/search
include ../time/time
include ../location/location
include ../double-arrows/double-arrows

mixin header(data)
    +b('header').header
        +e.title Go serf
        nav.nav
            +brand(brand)
            +nav-list(navList)
            +search()
            +time(time)
            +location(location)
        +double-arrows(doubleArrows)
        +main-slider(mainSlider)
        +main-subslider(mainSubslider)

